#!/bin/bash -l

#SBATCH --gres=gpu:1
#SBATCH --nodes=1
#SBATCH --partition=gh200
#SBATCH --ntasks-per-node=1
#SBATCH --cpus-per-task=48
#SBATCH --time=00:30:00
#SBATCH -o ./slurm.%N.%j.out
#SBATCH -e ./slurm.%N.%j.err

#SBATCH --nodelist=lynx-gh01

. /etc/profile.d/modules.sh
# module list
module load nvhpc

make

mpirun ./main
